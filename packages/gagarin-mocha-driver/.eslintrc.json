{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "semi": 2,
    "no-undef": "error",
    "no-unused-vars": "warn",
    "prefer-const": "warn",
    "no-const-assign": "error"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "globals": {
    "Package": false,
    "Npm": false
  },
  "plugins": [
  ]
}
